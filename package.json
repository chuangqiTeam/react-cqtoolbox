{
  "name": "react-cqtoolbox",
  "version": "1.0.11",
  "main": "lib",
  "scripts": {
    "start": "node ./scripts/start.js",
    "build": "flow && rm -rf ./lib && gulp",
    "flow": "flow server",
    "doc": "rm -rf ./docs && webpack --config ./config/webpack.pro.js",
    "test": "jest",
    "test:watch": "jest --watch --no-watchman",
    "lint": "eslint components ./.eslintrc --ext '.js,.jsx'",
    "release": "npm run build && cqgit"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-cq": "^0.0.8",
    "babel-preset-cqaso-kit": "^0.0.3",
    "babel-react-render-defender": "^1.1.1",
    "chalk": "^1.1.3",
    "chunk-manifest-webpack-plugin": "^1.0.0",
    "css-loader": "^0.26.1",
    "cssnext": "^1.8.4",
    "enzyme": "^2.8.0",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^3.19.0",
    "eslint-config-cqaso-kit": "^0.0.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "file-loader": "^0.10.0",
    "flow-bin": "^0.43.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-postcss": "^6.4.0",
    "happypack": "^3.0.2",
    "html-webpack-plugin": "^2.24.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "normalize.css": "^5.0.0",
    "postcss": "^5.1.2",
    "postcss-cssnext": "^2.8.0",
    "postcss-loader": "^1.2.1",
    "postcss-mixins": "^5.2.0",
    "postcss-nested": "^1.0.0",
    "postcss-smart-import": "^0.6.6",
    "postcss-url": "^5.1.2",
    "pre-commit": "^1.2.2",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dev-utils": "^0.4.2",
    "react-dom": "^15.4.1",
    "regenerator-runtime": "^0.10.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dev-server": "^2.2.0",
    "why-did-you-update": "^0.0.8"
  },
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "codemirror": "^5.14.2",
    "cq-kit-dom": "^0.0.3",
    "css-animation": "^1.3.2",
    "dom-align": "^1.5.3",
    "moment": "^2.18.1",
    "object-assign": "^4.1.0",
    "promise": "^7.1.1",
    "ramda": "^0.23.0",
    "rc-animate": "^2.3.3",
    "react-css-themr": "^2.0.0",
    "react-date-range": "^0.9.3",
    "react-ultimate-pagination": "^1.0.0",
    "warning": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^0.14 || ~15.4.0",
    "react-dom": "^0.14.0 || ~15.4.0"
  },
  "jest": {
    "testRegex": "(\\.|/)(__test__|spec)\\.jsx?$",
    "collectCoverageFrom": [
      "components/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css$)": "identity-obj-proxy"
    },
    "modulePaths": [
      "<rootDir>"
    ]
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
